create 536870912 Disk.img
format
mkdir /sys
mkdir /sys/bin
mkdir /sys/lib
mkdir /sys/resources
add /sys/resources/wallpaper.bmp Disk/sys/resources/wallpaper.bmp
add /sys/boot.ini Disk/sys/boot.ini
mkdir /users
mkdir /users/root
mkdir /users/public
add /testdoc.txt Disk/testdoc.txt
add /testdoc2.txt Disk/testdoc2.txt
save
unload
exit